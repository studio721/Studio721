{
    "name": "Project 721",
    "version": "1.0",
    "description": "Improves video conferencing during Google Meet",
    "manifest_version": 2,

    "background":{
        "scripts":["background.js"]
    },

    "content_scripts":[
        {
            "matches":["https://meet.google.com/*"],
            "js":["content.js"]
        }
    ],

    "browser_action": {
    "default_icon": "icon.png"
  }
}

